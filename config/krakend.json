{
   "version":2,
   "timeout":"3s",
   "cache_ttl":"3s",
   "name":"gateway-insuragility-v1.0",
   "port":"8090",
   "read_timeout":"30s",
   "write_timeout":"300s",
   "idle_timeout":"30s",
   "read_header_timeout":"1s",
   "host":[
      "http://gateway.docker.internal:8080",
      "http://workflow-app:8086"
   ],
  "extra_config":{
    "github_com/devopsfaith/krakend-cors":{
      "allow_origins":[],
      "allow_methods":[
        "GET",
        "HEAD",
        "POST",
        "PUT",
        "DELETE",
        "CONNECT",
        "OPTIONS",
        "TRACE",
        "PATCH"
      ],
      "allow_headers":[
        "Origin",
        "Authorization",
        "Content-Type",
        "Access-Control-Allow-Origin",
        "Locale",
        "Accept",
        "X-Requested-With",
        "Cache-Control",
        "Postman-Token",
        "X-Tenant",
        "X-User-Id",
        "X-Correlation-Id",
        "X-Lang"
      ],
      "allow_credentials":true,
      "max_age":"1s",
      "expose_headers":[
        "Origin",
        "Authorization",
        "Content-Type",
        "Access-Control-Allow-Origin",
        "Locale",
        "Accept",
        "X-Requested-With",
        "Cache-Control",
        "Postman-Token",
        "X-Tenant",
        "X-User-Id",
        "X-Correlation-Id"
      ]
    }
  },
  "endpoints":[
    {
      "endpoint":"/hello",
      "backend":[
        {
          "url_pattern":"/"
        }
      ],
      "headers_to_pass":[ "X-Tenant",  "X-User-Id", "X-Correlation-Id" ],
      "method":"GET",
      "output_encoding":"json",
      "extra_config":{
        "github.com/devopsfaith/krakend/proxy":{
          "static":{
            "data":{
              "response":"Hello from KrakenD!"
            },
            "strategy":"allways"
          }
        }
      }
    },
    {
      "endpoint":"/api/version",
      "backend":[
        {
          "url_pattern":"/"
        }
      ],
      "headers_to_pass":[ "X-Tenant",  "X-User-Id", "X-Correlation-Id" ],
      "method":"GET",
      "output_encoding":"json",
      "extra_config":{
        "github.com/devopsfaith/krakend/proxy":{
          "static":{
            "data":{
              "response":"2019.1"
            },
            "strategy":"allways"
          }
        }
      }
    },
    {{ include "users-authentication-v4.tmpl" }},
    {{ include "users-authorization-users-v4.tmpl" }}

  ]
}